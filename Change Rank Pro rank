local player = game.Players.LocalPlayer

-- Warten bis leaderstats geladen sind
local leaderstats = player:WaitForChild("leaderstats")

local pro = leaderstats:WaitForChild("Pro")
local rank = leaderstats:WaitForChild("Rank")
local rep = leaderstats:WaitForChild("Rep")

-- Werte Ã¤ndern
pro.Value = "F3"        -- Pro Rank
rank.Value = "MAX-59"  -- Rank
rep.Value = 0          -- Rep
